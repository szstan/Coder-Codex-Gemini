# 环境变量配置模板
# 复制此文件为 .env 并填写实际值

# ==================== 数据库配置 ====================
# PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_dev
DB_USER=postgres
DB_PASSWORD=change_me

# ==================== 缓存配置 ====================
# Redis 缓存配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=change_me
REDIS_DB=0

# ==================== 消息队列配置 ====================
# RabbitMQ 消息队列配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=change_me

# ==================== API 密钥 ====================
# 应用密钥
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# ==================== 应用配置 ====================
# 应用环境 (development/production)
APP_ENV=development
# 应用端口
APP_PORT=3000
